<Window x:Class="API_Animal_Pics.Windows.Saved_pics"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:API_Animal_Pics.Windows"
        Icon="pack://application:,,,/Resources/Indexicon.ico"
        mc:Ignorable="d"
        Title="Saved_pics" Height="450" Width="830">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="13*"/>
        </Grid.RowDefinitions>
        <Border Background="#312113" Grid.Row="1"/>
        <Menu Grid.Row="0" Grid.ColumnSpan="1">
            <MenuItem Header="_File" FontSize="20">
                <MenuItem Header="_Save" FontSize="20" Click="Save_Click"/>
                <MenuItem Header="_Close" FontSize="20" Click="Close_Click"/>
            </MenuItem>
        </Menu>
        <TextBlock Name="List_Name" FontSize="20" Text="List name" FontWeight="ExtraBold" HorizontalAlignment="Center" Grid.Row="1" Foreground="White"/>
        <Image Name="Table" Source="/Resources/Images/Table_Top.png" Stretch="Fill" Grid.Row="2"/>
        <ScrollViewer Grid.Row="2">
        <WrapPanel Name="ImageSet" Grid.Row="2"></WrapPanel>
        </ScrollViewer>
    </Grid>
</Window>
